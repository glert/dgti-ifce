{% extends "baselogin.djhtml" %}

{% load bootstrap_toolkit %}

{% block extra_head %}
    {{ form.media }}
{% endblock %}

{% block content %}

    <h1>Acompanhamento de Requisições</h1>
    
<br><br>
<div class="row">
    {% for s in requisicoes %}
    <div class="span4">
            <h2>{{ s }}</h2>

            <p>{{ s.getLastMessage }}</p>

            <p><a class="btn" href="/accounts/dialogo/">View details &raquo;</a></p>
        </div>
             
    {% endfor %}
    
</div>
    
{% endblock %}